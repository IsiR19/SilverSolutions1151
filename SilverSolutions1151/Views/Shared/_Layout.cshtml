<!DOCTYPE html>
<html lang="en" ng-app="myApp">
@RenderSection("Header", false)
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SilverSolutions1151</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/SilverSolutions1151.styles.css" asp-append-version="true" />
    <link href="~/Template/SilverSolutions1151/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link href="/Template/SilverSolutions1151/css/sb-admin-2.min.css" rel="stylesheet" asp-append-version="true" />
    <script src="~/Template/SilverSolutions1151/vendor/jquery/jquery.min.js" asp-append-version="true"></script>
</head>
<body>
    <div class="bg-primary text-white py-3">
        <div class="container">
            <h1 class="mb-0">Silver Solutions</h1>
        </div>
    </div>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/template/silversolutions1151/js/sb-admin-2.js" asp-append-version="true"></script>
    <script src="~/template/silversolutions1151/js/sb-admin-2.min.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
